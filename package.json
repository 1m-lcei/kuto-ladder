    "@vitejs/plugin-react": "^5.0.3",
    "daisyui": "^5.1.26",
    "gh-pages": "^6.3.0",
    "globals": "^16.4.0",
    "tailwindcss": "^4.1.14",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "vite": "npm:rolldown-vite@7.1.12"
